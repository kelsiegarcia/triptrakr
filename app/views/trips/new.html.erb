<h1>Create a new trip!</h1>
<%= form_for @trip do |f| %>

  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :beginning_date %>
  <%= f.date_field :date_beginning %>

  <%= f.label :end_date %>
  <%= f.date_field :date_end %>

  <%= f.label :description %>
  <%= f.text_area :description %>

 <%= f.submit %>
<% end %>